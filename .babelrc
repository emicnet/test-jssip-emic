{
    "env": {
        "development": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "chrome": "66",
                            "firefox": "60",
                            "safari": "11"
                        },
                        "modules": "commonjs",
                        "debug": true
                    }
                ]
            ]
        },
        "production": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "chrome": "66",
                            "firefox": "60",
                            "safari": "11"
                        },
                        "debug": true
                    }
                ]
            ],
            "plugins": [
                [
                    "transform-runtime",
                    {
                        "polyfill": false,
                        "regenerator": true
                    }
                ]
            ]
        }
    }
}
